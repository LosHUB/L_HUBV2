			FileName = game.PlaceId
		},
		Discord = {
			Enabled = true,
			Invite = "7TC8dchz6C",
			RememberJoins = true
		}
	})



	local main = Window:CreateTab("Farm", 4483362458) -- Title, Image



	local Button = main:CreateButton({
		Name = "INFINITE STATS",
		Info = "INFINITE STATS", -- Speaks for itself, Remove if none.
